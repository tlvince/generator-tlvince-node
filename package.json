{
  "name": "generator-tlvince-node",
  "description": "Scaffold out a node module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tlvince/generator-tlvince-node.git"
  },
  "author": "Tom Vincent <github@tlvince.com> (https://github.com/tlvince)",
  "engines": {
    "node": ">=5"
  },
  "scripts": {
    "pretest": "standard",
    "test": "mocha",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "files": [
    "app"
  ],
  "keywords": [
    "yeoman-generator",
    "plugin",
    "boilerplate",
    "template",
    "scaffold",
    "node",
    "module",
    "node_module",
    "node-module",
    "init",
    "tlvince"
  ],
  "dependencies": {
    "generator-license": "4.0.0",
    "generator-readme": "1.1.0",
    "inquirer-npm-name": "2.0.0",
    "lodash.camelcase": "4.3.0",
    "lodash.kebabcase": "4.1.1",
    "normalize-url": "1.6.1",
    "yeoman-generator": "0.24.1"
  },
  "devDependencies": {
    "ghooks": "1.3.2",
    "mocha": "3.0.2",
    "semantic-release": "4.3.5",
    "standard": "8.0.0",
    "validate-commit-msg": "2.8.0",
    "yeoman-assert": "2.2.1",
    "yeoman-test": "1.5.0"
  },
  "standard": {
    "ignore": [
      "app/templates"
    ]
  },
  "bugs": {
    "url": "https://github.com/tlvince/generator-tlvince-node/issues"
  },
  "homepage": "https://github.com/tlvince/generator-tlvince-node#readme",
  "main": "test.js",
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-push": "npm test"
    }
